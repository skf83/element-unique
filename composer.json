{
    "name": "element/unique",
    "description": "Element package for generating unique ID's such as software licenses, auth tokens, OTP's",
    "keywords": ["Element core", "Auth-Recovery", "Token", "Software license", "Uid", "generator"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Korfitz",
            "homepage": "https://www.korfitz.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "stefan@korfitz.com"
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "php8": "This package utilizes features that are found in 7.4 - But if you use php8 or higher, then you should have no problems"
    },
    "require": {
        "slim/slim": "~3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Element\\Unique\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Element\\Unique\\Tests\\": "tests/"
        }
    }
}
